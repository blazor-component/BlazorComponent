@namespace BlazorComponent
@typeparam TValue
@inherits BInputInputSlot<TValue,ITextField<TValue>>

<div class="@CssProvider.GetClass("input-slot")"
     style="@CssProvider.GetStyle("input-slot")"
     @onexclick="HandleOnClickAsync"
     @onexmouseup="HandleOnMouseUpAsync"
     @attributes="InputSlotAttrs"
     @ref="InputSlotRef">
     @RenderPart(typeof(BTextFieldPrependInnerSlot<,>))
     @RenderPart(typeof(BInputDefaultSlot<,>))
</div>
